#!/edx/app/edxapp/venvs/edxapp/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'pysrt==0.4.7','console_scripts','srt'
__requires__ = 'pysrt==0.4.7'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('pysrt==0.4.7', 'console_scripts', 'srt')()
    )
